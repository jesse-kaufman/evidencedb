include /_utils/_title
include /_utils/_description
    video:video
        video:thumbnail_loc
            | #{process.env.CDN}videos/#{`${encodeURIComponent(item.filename)}`}.webp
        video:title
            | #{title}
        video:description
            | #{description}
        video:content_loc
            | #{process.env.CDN}videos/#{`${encodeURIComponent(item.filename)}`}
        video:player_loc
            | #{process.env.CDN}videos/#{`${encodeURIComponent(item.filename)}`}
        video:duration
            | #{item.durationSeconds}
        video:family_friendly no